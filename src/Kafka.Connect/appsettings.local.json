          "properties": {
            "host": "localhost",
            "port": 5432,
            "userId": "postgres",
            "password": "postgres",
            "database": "kafka_connect_db",
            "schema": "public",
            "table": "sink_table",
            "lookup": "\"userId\"='#userId#'",
            "filter": "\"userId\"='#userId#'"
