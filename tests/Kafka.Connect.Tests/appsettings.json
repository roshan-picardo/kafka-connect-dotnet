{
  "DetailedLog": false,
  "RawJsonLog": true,
  "SkipInfrastructure": false,
  "SkipKafkaConnectLogFlush": true,
  "TestContainers": {
    "Network": {
      "Name": "local"
    },
    "Containers": [
      {
        "Image": "confluentinc/cp-zookeeper:7.6.0",
        "Name": "zookeeper",
        "Hostname": "zookeeper",
        "Ports": [
          "2181"
        ],
        "Environment": {
          "ZOOKEEPER_CLIENT_PORT": "2181",
          "ZOOKEEPER_TICK_TIME": "2000"
        }
      },
      {
        "Image": "confluentinc/cp-server:7.6.0",
        "Name": "broker",
        "Hostname": "broker",
        "Ports": [
          "9092",
          "9093"
        ],
        "Environment": {
          "KAFKA_BROKER_ID": "1",
          "KAFKA_ZOOKEEPER_CONNECT": "zookeeper:2181",
          "KAFKA_LISTENER_SECURITY_PROTOCOL_MAP": "PLAINTEXT:PLAINTEXT,PLAINTEXT_HOST:PLAINTEXT",
          "KAFKA_LISTENERS": "PLAINTEXT://0.0.0.0:9092,PLAINTEXT_HOST://0.0.0.0:9093",
          "KAFKA_ADVERTISED_LISTENERS": "PLAINTEXT://broker:9092,PLAINTEXT_HOST://127.0.0.1:9093",
          "KAFKA_METRIC_REPORTERS": "io.confluent.metrics.reporter.ConfluentMetricsReporter",
          "KAFKA_OFFSETS_TOPIC_REPLICATION_FACTOR": "1",
          "KAFKA_GROUP_INITIAL_REBALANCE_DELAY_MS": "0",
          "KAFKA_CONFLUENT_LICENSE_TOPIC_REPLICATION_FACTOR": "1",
          "KAFKA_CONFLUENT_BALANCER_TOPIC_REPLICATION_FACTOR": "1",
          "KAFKA_TRANSACTION_STATE_LOG_MIN_ISR": "1",
          "KAFKA_TRANSACTION_STATE_LOG_REPLICATION_FACTOR": "1",
          "KAFKA_JMX_PORT": "9101",
          "KAFKA_JMX_HOSTNAME": "localhost",
          "CONFLUENT_METRICS_REPORTER_BOOTSTRAP_SERVERS": "broker:9092",
          "CONFLUENT_METRICS_REPORTER_TOPIC_REPLICAS": "1",
          "CONFLUENT_METRICS_ENABLE": "true",
          "CONFLUENT_SUPPORT_CUSTOMER_ID": "anonymous"
        }
      },
      {
        "Image": "confluentinc/cp-schema-registry:7.6.0",
        "Name": "schema",
        "Hostname": "schema",
        "Ports": [
          "8081"
        ],
        "Environment": {
          "SCHEMA_REGISTRY_HOST_NAME": "schema",
          "SCHEMA_REGISTRY_KAFKASTORE_BOOTSTRAP_SERVERS": "broker:9092",
          "SCHEMA_REGISTRY_LISTENERS": "http://0.0.0.0:8081"
        }
      },
      {
        "Image": "mongo:7.0",
        "Name": "mongodb",
        "Hostname": "mongodb",
        "Ports": [
          "27017"
        ],
        "Command": [
          "--replSet",
          "rs0",
          "--bind_ip_all"
        ],
        "WaitForHealthCheck": false,
        "StartupTimeoutSeconds": 120,
        "HealthCheck": {
          "Test": [
            "CMD-SHELL",
            "mongosh --quiet --eval \"try { rs.status(); } catch (err) { rs.initiate({ _id: 'rs0', members: [{ _id: 0, host: 'mongodb:27017' }] }); }\""
          ],
          "Interval": "10s",
          "Timeout": "10s",
          "Retries": 10,
          "StartPeriod": "40s"
        }
      },
      {
        "Image": "postgres:16",
        "Name": "postgres",
        "Hostname": "postgres",
        "Ports": [
          "5432"
        ],
        "Environment": {
          "POSTGRES_DB": "kafka_connect_db",
          "POSTGRES_USER": "postgres",
          "POSTGRES_PASSWORD": "postgres"
        }
      },
      {
        "Image": "mcr.microsoft.com/mssql/server:2022-latest",
        "Name": "sqlserver",
        "Hostname": "sqlserver",
        "Ports": [
          "1433"
        ],
        "Environment": {
          "ACCEPT_EULA": "Y",
          "SA_PASSWORD": "YourStrong@Passw0rd",
          "MSSQL_PID": "Express"
        }
      },
      {
        "Image": "mysql:8.0",
        "Name": "mysql",
        "Hostname": "mysql",
        "Ports": [
          "3306"
        ],
        "Environment": {
          "MYSQL_ROOT_PASSWORD": "rootpassword",
          "MYSQL_DATABASE": "kafka_connect_db"
        }
      },
      {
        "Image": "gvenzl/oracle-xe:21-slim",
        "Name": "oracle",
        "Hostname": "oracle",
        "Ports": [
          "1521"
        ],
        "Environment": {
          "ORACLE_PASSWORD": "oracle123",
          "APP_USER": "kafka_user",
          "APP_USER_PASSWORD": "kafka_password"
        }
      },
      {
        "Image": "amazon/dynamodb-local:latest",
        "Name": "dynamodb",
        "Hostname": "dynamodb",
        "Ports": [
          "8000"
        ]
      },
      {
        "Name": "worker",
        "Hostname": "worker",
        "NetworkAliases": [
          "worker"
        ],
        "Ports": [
          "6000"
        ],
        "DockerfilePath": "local.Dockerfile",
        "ConfigurationsPath": "Configurations",
        "WaitForHealthCheck": true,
        "HealthCheckEndpoint": "/health",
        "StartupTimeoutSeconds": 120,
        "Enabled": true,
        "CleanUpImage": false,
        "Command": [
          "--config=/app/config/appsettings.json",
          "--config=/app/config/appsettings.mongodb.json",
          "--config=/app/config/appsettings.postgres.json",
          "--config=/app/config/appsettings.sqlserver.json",
          "--config=/app/config/appsettings.mysql.json",
          "--config=/app/config/appsettings.oracle.json",
          "--config=/app/config/appsettings.dynamodb.json"
        ],
        "BindMounts": {
          "Configurations": "/app/config"
        }
      }
    ],
    "Producer": {
      "ClientId": "testcontainers-producer"
    },
    "Consumer": {
      "GroupId": "testcontainers-consumer-group",
      "AutoOffsetReset": "Earliest",
      "EnableAutoCommit": false
    }
  },
  "Services": {
    "Kafka": "127.0.0.1:9093",
    "SchemaRegistry": "http://127.0.0.1:8081",
    "Mongo": "mongodb://127.0.0.1:27017/?replicaSet=rs0&directConnection=true",
    "Postgres": "Host=127.0.0.1;Port=5432;Database=kafka_connect_db;Username=postgres;Password=postgres",
    "SqlServer": "Server=127.0.0.1,1433;Database=kafka_connect_db;User Id=sa;Password=YourStrong@Passw0rd;TrustServerCertificate=true;",
    "MySql": "Server=127.0.0.1;Port=3306;Database=kafka_connect_db;Uid=root;Pwd=rootpassword;",
    "Oracle": "Data Source=(DESCRIPTION=(ADDRESS=(PROTOCOL=TCP)(HOST=127.0.0.1)(PORT=1521))(CONNECT_DATA=(SERVICE_NAME=XE)));User Id=system;Password=oracle123;",
    "DynamoDb": "http://localhost:8000",
    "Worker": "http://localhost:6000"
  }
}