            "schema": "\"SYSTEM\"",
            "table": "\"sink_table\"",
            "lookup": "\"userId\"='#userId#'",
            "filter": "\"userId\"='#userId#'"
          }
        },
        "log": {
          "provider": "",
          "attributes": [
            "timestamp",
            "operation",
            "after.id",
            "after.name",
