{
  "worker": {
    "connectors": {
      "postgres-sink-connector": {
        "groupId": "postgres-sink-connector",
        "topics": ["postgres-sink-data-topic"],
        "tasks": 1,
        "paused": false,
        "disabled": false,
        "processors": {
          "1": {
            "name": "Kafka.Connect.Processors.WhitelistFieldProjector",
            "settings": [
              "userId",
              "userName",
              "email",
              "timestamp",
              "action"
            ]
          }
        },
        "plugin": {
          "type": "sink",
          "name": "postgres",
          "handler": "Kafka.Connect.Postgres.PostgresPluginHandler",
          "strategy": {
            "Name": "Kafka.Connect.Postgres.Strategies.UpsertStrategy"
          },
          "properties": {
            "host": "postgres",
            "port": 5432,
            "userId": "postgres",
            "password": "postgres",
            "database": "kafka_connect_db",
            "schema": "public",
            "table": "sink_table",
            "lookup": "\"userId\"='#userId#'",
            "filter": "\"userId\"='#userId#'"
          }
        },
        "log": {
          "provider": "",
          "attributes": [
            "userId",
            "userName"
          ]
        }
      },
      "postgres-source-connector": {
        "groupId": "postgres-source-connector",
        "tasks": 1,
        "paused": false,
        "disabled": false,
        "plugin": {
          "type": "source",
          "name": "postgres",
          "handler": "Kafka.Connect.Postgres.PostgresPluginHandler",
          "strategy": {
            "name": "Kafka.Connect.Postgres.Strategies.ReadStrategy"
          },
          "properties": {
            "host": "postgres",
            "port": 5432,
            "userId": "postgres",
            "password": "postgres",
            "database": "kafka_connect_db",
            "changelog": {
              "schema": "public",
              "table": "kafka_connect_audit"
            },
            "commands": {
              "read-source-table": {
                "topic": "postgres-source-data-topic",
                "table": "source_table",
                "schema": "public",
                "keys": ["userId"],
                "snapshot": {
                  "enabled": false,
                  "key": "userId"
                }
              }
            }
          }
        },
        "log": {
          "provider": "",
          "attributes": [
            "id",
            "timestamp",
            "operation",
            "after.userId",
            "after.userName",
            "after.email"
          ]
        }
      }
    }
  }
}